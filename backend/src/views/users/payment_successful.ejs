<script>
  var count = 0;
  var data = [];
  var arr = window.location.href;
  for (var i = 0; i < arr.length; i++) {
    if (arr[i-1] == "?") {
      count++;
    }
    if (count == 1) {
      data.push(arr[i]);
    }
  }
  var has=data.join("")
 console.log(has)
  function run(){
    window.location.href=`landingPage?${has}`
  }
</script>
